pipeline {
  agent any
  stages{
       stage('Test stage'){
             steps{
                  withMaven(maven : 'mven_6_0'){
                    sh 'mvn test'
               }
          }
      }
      stage ('deployment stage'){
        steps{
             withMaven(maven :: 'mven_6_0'){
                    sh 'mvn deploy'
               }
          }
      }
